package com.hms.roommanagement;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.cloud.client.loadbalancer.LoadBalanced;
import org.springframework.cloud.netflix.eureka.EnableEurekaClient;


@SpringBootApplication
@EnableEurekaClient
@OpenAPIDefinition(info=@Info(title="Inventory API",version="1.0",description="An API used for Employeess."))
public class RoomManagementApplication {

	@LoadBalanced
	public static void main(String[] args) {
		SpringApplication.run(RoomManagementApplication.class, args);
	}

}
